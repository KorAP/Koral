{
   "@context":{
      "korap":"http://korap.ids-mannheim.de/ns/KorAP/json-ld/v0.1/",
      "boundary":"korap:boundary/",
      "group":"korap:group/",
      "operation":{
         "@id":"group:operation/",
         "@type":"@id"
      },
      "class":{
         "@id":"group:class",
         "@type":"xsd:integer"
      },
      "operands":{
         "@id":"group:operands",
         "@container":"@list"
      },
      "frame":{
         "@id":"group:frame/",
         "@type":"@id"
      },
      "classRef":{
         "@id":"group:classRef",
         "@type":"xsd:integer"
      },
      "spanRef":{
         "@id":"group:spanRef",
         "@type":"xsd:integer"
      },
      "classRefOp":{
         "@id":"group:classRefOp",
         "@type":"@id"
      },
      "min":{
         "@id":"boundary:min",
         "@type":"xsd:integer"
      },
      "max":{
         "@id":"boundary:max",
         "@type":"xsd:integer"
      },
      "exclude":{
         "@id":"group:exclude",
         "@type":"xsd:boolean"
      },
      "distances":{
         "@id":"group:distances",
         "@container":"@list"
      },
      "inOrder":{
         "@id":"group:inOrder",
         "@type":"xsd:boolean"
      }
   },
   "query":{
      "type":"group",
      "operation":"sequence",
      "inOrder":"true",
      "distances":[
         {
            "type":"distance",
            "measure":"w",
            "min":1,
            "max":3
         }
      ],
      "operands":[
         {
            "type":"token",
            "key":{
               "type":"term",
               "key":"Der",
               "layer":"orth",
               "match":"eq"
            }
         },
         {
            "type":"token",
            "key":{
               "type":"term",
               "key":"Mann",
               "layer":"orth",
               "match":"eq"
            }
         }
      ]
   }
}